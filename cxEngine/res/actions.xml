<?xml version="1.0" encoding="UTF-8"?>

<cxActionSet cxActionSet.type="multiple" cxActionSet.onLoad="actionsLoad">
    <cxMove id="move"
        cxAction.speed="1.0"
        cxAction.time="1.5"
        cxMove.x="300"
        cxMove.y="350"
        cxAction.assist="50"
        cxAction.split="10"
        cxAction.curve="ElasticOut"/>
    <cxMove id="reset"
        cxAction.speed="1.0"
        cxAction.time="1.5"
        cxMove.x="-300"
        cxMove.y="350"
        cxAction.assist="10"
        cxAction.curve="ElasticOut"/>
    <cxRotate id="rotate"
        cxAction.speed="1.0"
        cxAction.time="3.0"
        cxRotate.z="360"
        cxAction.curve="cxCurveCubeIn"/>
    <cxScale id="scale2"
        cxAction.id="t1"
        cxAction.speed="1.0"
        cxAction.time="0.5"
        cxScale.x="1.3"
        cxScale.y="1.3"/>
    <cxScale id="scale3"
        cxAction.speed="1.0"
        cxAction.time="0.5"
        cxScale.x="1.0"
        cxScale.y="1.0"/>
    <cxJump id="jump"
        cxAction.time="5"
        cxJump.position="300,0"
        cxJump.jumps="8"
        cxJump.height="100"/>
    <cxFade id="fade1"
        cxAction.time="3"
        cxFade.alpha="0"/>
    <cxFade id="fade2"
        cxAction.time="3"
        cxFade.alpha="1"/>
    <cxTint id="tint1"
        cxAction.time="3"
        cxTint.color="1,0,0"/>
    <cxTimer id="timer"
        cxAction.time="2"
        cxTimer.repeat="5"
        cxTimer.onArrive="cxLogger('timer')" />
</cxActionSet>
